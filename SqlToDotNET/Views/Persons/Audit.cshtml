@model IEnumerable<SqlToDotNET.Models.PersonAudit>

@{
    ViewData["Title"] = "Audit";
}


<div class="card m-4 row col-lg-6 mx-auto">
    <div class="card-header">
        <h2 class="alert alert-success text-center">Person Audit List</h2>
    </div>
    <div class="card-body">
        <table class="table table-striped table-bordered">
            <thead class="table-success">
                <tr>
                    <th class="text-center">
                        @Html.DisplayNameFor(x => x.AuditData)
                    </th>
                </tr>
            </thead>

            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(x => item.AuditData)
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>

</div>
